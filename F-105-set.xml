<PropertyList>

 <sim>

  <description>Republic F-105D Thunderchief</description>
  <author>David Culp, Julien Pierru, "pebble garden"</author>
  <status>beta v0.4</status>
  <flight-model>jsb</flight-model>
  <aero>F-105D</aero>
  
  <startup>
   <splash-texture>Aircraft/F-105/F-105-splash.png</splash-texture>
  </startup>
  
   <systems>
	<electrical>
        <path>Aircraft/Generic/generic-electrical.xml</path>
      </electrical>
      <autopilot>
        <path>Aircraft/F-100D/Systems/autopilot5.xml</path>
      </autopilot>
      <autopilot>
        <path>Aircraft/F-105/Systems/engine-nozzle.xml</path>
      </autopilot>
       <refuel>
         <type>probe</type>
         <serviceable type="bool">true</serviceable>
       </refuel>
	   
	    </systems>

  <gui>
   <style>
    <colors>
     <splash-screen>
      <red>0</red>
      <green>0.05</green>
      <blue>0</blue>
     </splash-screen>
     <splash-font>
      <red>0.31</red>
      <blue>0.19</blue>
      <green>0.4</green>
     </splash-font>
    </colors>
   </style>
  </gui>

  <sound>
    <path>Aircraft/F-105/Sounds/F-105-sound.xml</path>
  </sound>

  <submodels> 
    <serviceable type="bool">true</serviceable>
    <path>Aircraft/F-105/submodels.xml</path>
  </submodels>
  
    <ai>
      <scenario>
	   <path>Aircraft/F-105/AI/EGOD_arrestor_cables.xml</path>
	  </scenario>
    </ai>

  <virtual-cockpit archive="y">true</virtual-cockpit>
  <allow-toggle-cockpit archive="y">false</allow-toggle-cockpit>

  <panel>
    <visibility archive="y">false</visibility>
  </panel>

  <view n="0">
   <internal archive="y">true</internal>
   <config>
    <pitch-offset-deg>-3.1</pitch-offset-deg>
    <x-offset-m archive="y">0.0</x-offset-m><!-- right/left -->
    <y-offset-m archive="y">1.2</y-offset-m><!-- up/down -->
    <z-offset-m archive="y">-4.25</z-offset-m><!-- aft/fore -->
   </config>
  </view>
  
     <view n="100">
	   <name>Pilot cam</name>
        <internal archive="y">false</internal>
        <config>
		 <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <x-offset-m archive="y">0.0</x-offset-m><!-- right/left -->
    <y-offset-m archive="y">1.2</y-offset-m><!-- up/down -->
    <z-offset-m archive="y">-4.9</z-offset-m><!-- aft/fore -->
          <pitch-offset-deg>0</pitch-offset-deg>
		  <heading-offset-deg>180</heading-offset-deg>
          <default-field-of-view-deg>95</default-field-of-view-deg>
        </config>
      </view>


		<view n="101">
			<name>Over the shoulder</name>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<x-offset-m archive="y">0.29</x-offset-m>
				<y-offset-m archive="y">1.2</y-offset-m>   <!-- 0.51-->
                <z-offset-m archive="y">-3.9</z-offset-m>   <!-- 2.62-->
				<pitch-offset-deg>-3</pitch-offset-deg>
				<heading-offset-deg>10</heading-offset-deg>
				<default-field-of-view-deg>65</default-field-of-view-deg>
			</config>
		</view>



  <model>
   <path>Aircraft/F-105/Models/F-105.xml</path>
  </model>

  <chase-distance-m>-22</chase-distance-m>

  <flaps>
   <setting>0.00</setting>
   <setting>0.49</setting> 
   <setting>1.00</setting> 
  </flaps>

  <help>
   <key>
    <name>c (lower case c) </name>
    <desc>Toggle canopy</desc>
   </key>
   <key>
    <name>Shift-Q</name>
    <desc>Fuel cutoff switches</desc>
   </key>
   <key>
    <name>n (lower case n) </name>
    <desc>Toggle tailhook</desc>
   </key>
   <key>
    <name>m (lower case m) </name>
    <desc>Nose wheel steering</desc>
   </key>
   <key>
    <name>s (lower case s)</name>
    <desc>Toggle starter valves</desc>
   </key>
   <key>
    <name>e (lower case e)</name>
    <desc>Release selected stores</desc>
   </key>
   <key>
    <name>d (lower case d)</name>
    <desc>Deploy drag 'chute</desc>
   </key>
   <key>
    <name>j (lower case j)</name>
    <desc>Jettison drag 'chute</desc>
   </key>
   <key>
    <name>1 </name>
    <desc>Select Stations 1 and 5, deselect others</desc>
   </key>
   <key>
    <name>2 </name>
    <desc>Select Stations 2 and 4, deselect others</desc>
   </key>
   <key>
    <name>3 </name>
    <desc>Select Station 3, deselect others</desc>
   </key>
   <key>
    <name>Shift 1 </name>
    <desc>Select Stations 1 and 5</desc>
   </key>
   <key>
    <name>Shift 2 </name>
    <desc>Select Stations 2 and 4</desc>
   </key>
   <key>
    <name>Shift 3 </name>
    <desc>Select Station 3</desc>
   </key>
   <key>
    <name>0 </name>
    <desc>Deselect All Stations</desc>
   </key>
   <line> </line>
   <line>Takeoff: full flaps, rotate at 195 kts.</line>
   <line>Climb  : 400 kts. / 0.84 mach</line>
   <line>Cruise : 0.92 mach</line> 
   <line>Landing: full flaps, land at 185 kts. + 3 kts. per 1000 lbs fuel.</line>
   <line> </line>
   <line>Gear and Flap limit: 275 knots.</line>
   <line> </line>
   <line>Clickable panel.  Hit Ctrl-C to see hot spots.</line>
   <line> </line>
   <line>Standard load: </line>
   <line>   Station 1:  ECM pod</line>
   <line>   Station 2:  450 gallon drop tank</line>
   <line>   Station 3:  6 X M-117, 750 lb. bombs</line>
   <line>   Station 4:  450 gallon drop tank</line>
   <line>   Station 5:  unused</line>
   <text>
   </text>
  </help>

  <rendering>
    <redout>
      <parameters>
        <blackout-onset-g>4.0</blackout-onset-g>
        <blackout-complete-g>8.0</blackout-complete-g>
      </parameters> 
    </redout>
  </rendering>

 <mp-systems>
<radar-enabled type="bool">true</radar-enabled>
<ecm-rwr-enabled type="bool">true</ecm-rwr-enabled>
<combat-enabled type="bool">true</combat-enabled>
</mp-systems>	
	

  <multiplay>
     <generic n="0">
	
	
      <!-- Nav lights on -->
      <int n="0" alias="/controls/lighting/nav-lights-switch" />
      <!-- Beacon on -->
      <int n="1" alias="/controls/lighting/beacon-switch" />
      
      <!-- int[2] reserved for radar -->
      <int n="2" type="int">1</int>    <!-- force radar standby transmission over MP -->
      <!-- Landing lights on -->
      <int n="3" alias="/controls/lighting/landing-lights-switch" />
	  
	    <int n="5" alias="/engines/engine[0]/state" />
	 
	  <int n="7" alias="/sim/crashed" />
      
       
	<int n="9" type="int">  0 </int>  <!-- RESERVED FOR DUAL CONTROL (Boolean Light Switch)-->
       
	 <float n="8" alias="/engines/engine[0]/augmentation" /> <!-- Reheat Left Engine -->
	 
	<float n="15" alias="/velocities/mach" /> <!-- Mach for Multiplayer Sound -->
	 <float n="16" alias="/engines/engine[0]/thrust-lbs" /> <!-- Thrust for Multiplayer Sound -->
	
	
      <string n="0" type="string"></string>    <!-- bs_TDM1_mpp -->
				<string n="1" type="string"></string>    <!-- pilot's instruments -->
      </generic>
  </multiplay>
  
  
   <menubar include="Dialogs/menu.xml"/>

 </sim>
 
  
 <environment>
    <turbulence>
      <use-cloud-turbulence>true</use-cloud-turbulence>
    </turbulence>
  </environment>

 <controls>
  <flight>
   <flaps>0.0</flaps>
   <elevator-trim>-0.2</elevator-trim>
  </flight>
  <lighting>
   <instruments-norm>0.3</instruments-norm>
  </lighting>
  <armament>
   <gunsight>
    <power-on type="bool">true</power-on>
    <intensity type="double">0.3</intensity>
   </gunsight>
   <master-arm type="bool">false</master-arm>
  </armament>
  <gear>
      <brake-parking type="double">1.0</brake-parking>
    </gear>
 </controls>

 <engines>
  <engine n="0">
    <running>true</running>
  </engine>
 </engines>

<input>
  <keyboard>

 <key n="33">
  <name>Shift 1</name>
  <desc>Add Station 1</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station1/selected</property>
   <value type="int">1</value>
  </binding>
 </key>

 <key n="64">
  <name>Shift 2</name>
  <desc>Add Station 2</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station2/selected</property>
   <value type="int">1</value>
  </binding>
 </key>

 <key n="35">
  <name>Shift 3</name>
  <desc>Add Station 3</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station3/selected</property>
   <value type="int">1</value>
  </binding>
 </key>

 <key n="81">
  <name>Shift-Q</name>
  <desc>Fuel Cutoff Switch or Lever.</desc>
  <binding>
   <condition>
    <property>sim/input/selected/engine[0]</property>
   </condition>
   <command>property-toggle</command>
   <property>controls/engines/engine[0]/cutoff</property>
  </binding>
 </key>

    <key n="99">
     <name>c</name>
     <desc>toggle canopy</desc>
     <binding>
       <command>property-toggle</command>
       <property>fdm/jsbsim/systems/canopy/command</property>
     </binding>
    </key>

   <key n="100">
     <name>d</name>
     <desc>Deploy braking chute</desc>
     <binding>
      <condition>
       <equals>
         <property>controls/flight/chute_jettisoned</property>
         <value>0</value>
       </equals>
      </condition>
      <command>property-assign</command>
      <property>fdm/jsbsim/systems/chute/deploy</property>
      <value type="int">1</value>
     </binding>
    </key>

    <key n="106">
      <name>j</name>
      <desc>jettison braking chute</desc>
      <binding>
       <command>property-assign</command>
       <property>fdm/jsbsim/systems/chute/deploy</property>
       <value>0</value>
      </binding>
      <binding>
       <command>property-assign</command>
       <property>controls/flight/chute_jettisoned</property>
       <value>1</value>
      </binding>
    </key>

    <key n="109">
     <name>m</name>
     <desc>engage nose wheel steering</desc>
     <binding>
      <command>property-assign</command>
      <property>fdm/jsbsim/systems/NWS/engaged</property>
      <value>1</value> 
     </binding>
     <mod-up>
       <binding>
        <command>property-assign</command>
        <property>fdm/jsbsim/systems/NWS/engaged</property>
        <value>0</value> 
       </binding>
     </mod-up>
    </key>

    <key n="110">
      <name>n</name>
      <desc>Toggle tailhook.</desc>
      <binding>
       <command>property-toggle</command>
       <property>fdm/jsbsim/systems/hook/tailhook-cmd-norm</property>
      </binding>
    </key>

    <key n="115">
     <name>s</name>
     <desc>Toggle starter valves.</desc>
     <binding>
       <command>property-toggle</command>
       <property>controls/engines/engine[0]/starter</property>
     </binding>
     <binding>
       <command>property-toggle</command>
       <property>controls/engines/engine[1]/starter</property>
     </binding>
     <mod-up>
       <binding>
         <command>null</command>
       </binding>
     </mod-up>
    </key>

    <key n="101">
     <name>e</name>
     <desc>Release selected store(s)</desc>
      <binding>
       <command>property-assign</command>
       <property>fdm/jsbsim/systems/armament/release</property>
       <value>1</value>
      </binding>
     <mod-up>
       <binding>
        <command>property-assign</command>
        <property>fdm/jsbsim/systems/armament/release</property>
        <value>0</value> 
       </binding>
     </mod-up>
    </key>

 <key n="48">
  <name>0</name>
  <desc>De-select All Stations</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station1/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station2/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station3/selected</property>
   <value type="int">0</value>
  </binding>
 </key>

 <key n="49">
  <name>1</name>
  <desc>Select Stations 1 and 5 exclusively</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station1/selected</property>
   <value type="int">1</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station2/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station3/selected</property>
   <value type="int">0</value>
  </binding>
 </key>

 <key n="50">
  <name>2</name>
  <desc>Select Stations 2 and 4 exclusively</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station1/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station2/selected</property>
   <value type="int">1</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station3/selected</property>
   <value type="int">0</value>
  </binding>
 </key>

 <key n="51">
  <name>3</name>
  <desc>Select Station 3 exclusively</desc>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station1/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station2/selected</property>
   <value type="int">0</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>fdm/jsbsim/systems/armament/station3/selected</property>
   <value type="int">1</value>
  </binding>
 </key>
<key n="83">
    <name>S</name>
    <desc>Start smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>sim/multiplay/generic/int[6]</property>
      <value>true</value>
    </binding>
  </key>

  <key n="117">
    <name>u</name>
    <desc>Stop smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>sim/multiplay/generic/int[6]</property>
      <value>false</value>
    </binding>
  </key>

 </keyboard>
</input>


