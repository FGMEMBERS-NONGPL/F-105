 <!-- Landing Gear and Doors -->

 <animation>
  <type>select</type>
  <object-name>LTire</object-name>
  <object-name>RTire</object-name>
  <object-name>NoseGearStrut</object-name>
  <object-name>NoseGearDoorLeft</object-name>
  <object-name>NoseGearDoorRight</object-name>
  <object-name>LMainGearStrut</object-name>
  <object-name>LMainGearDoor-upper</object-name>
  <object-name>LMainGearDoor-lower</object-name>
  <object-name>LMainGearDoor-inner</object-name>
  <object-name>RMainGearStrut</object-name>
  <object-name>RMainGearDoor-upper</object-name>
  <object-name>RMainGearDoor-lower</object-name>
  <object-name>RMainGearDoor-inner</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[0]/position-norm</property>
    <value>0.1</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NTire</object-name>
  <object-name>NoseGearStrutLower</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[0]/position-norm</property>
    <value>0.1</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LMainGearBrace</object-name>
  <object-name>RMainGearBrace</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[0]/position-norm</property>
    <value>0.95</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NTire</object-name>
  <object-name>NoseGearStrut</object-name>
  <object-name>NoseGearStrutLower</object-name>
  <property>/gear/gear[0]/position-norm</property>
  <factor>-100</factor>
  <offset-deg>100</offset-deg>   
  <center>
   <x-m>-5.05</x-m>
   <y-m>0</y-m>
   <z-m>-0.77</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

