<!-- pilot -->
 <model>
 <name>pilot</name>
   <path>Aircraft/F-105/Models/pilots/pilot.xml</path>
   <offsets>
     <x-m>-4.3</x-m>
     <y-m>0</y-m>
     <z-m>0.62</z-m>
     <pitch-deg>0</pitch-deg>
   </offsets>
 </model>
 
 <!-- Suppress Seat/Pilot Models in Cockpit View -->
 <animation>
  <type>select</type>
  <object-name>pilot</object-name>
   <object-name>canopy-glass</object-name>
	<object-name>windscreen</object-name>
  <condition>
   <not>
   <equals>
    <property>sim/current-view/view-number</property>
    <value>0</value>
   </equals>
   </not>
  </condition>
 </animation>
 
 
