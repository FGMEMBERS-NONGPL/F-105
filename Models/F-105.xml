<!-- A wrapper file for the F-105 3D model -->

<PropertyList>

 <path>F105D.ac</path>

 
  <!-- MP sound call -->
  <sound><path>Aircraft/F-105/Sounds/mp-sound.xml</path></sound>
 
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/F-105/Models/Liveries", 10);
    </load>
    <unload>
      livery_update.stop();
    </unload>
  </nasal>
 
 <!-- fuselage -->
  <animation>
        <type>material</type>
        <object-name>fuselage</object-name>
		<object-name>refueling-bay-door</object-name>
        <object-name>camera</object-name>
		<object-name>probe-door-fwd</object-name>
		<object-name>probe</object-name>
		<object-name>pitot-tube</object-name>
		<object-name>canopy-frame</object-name>
		<object-name>scoop</object-name>
		<object-name>ventral-fin</object-name>
		
		<object-name>cooling-duct</object-name>
		<object-name>NoseGearDoorRight</object-name>
		<object-name>NoseGearDoorLeft</object-name>
		
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-fuselage</texture-prop>
        <texture>fuselage2.png</texture>
    </animation>
 
 
	
	
 
 
 <!-- wings -->
    <animation>
        <type>material</type>
		<object-name>LWing.001</object-name>
        <object-name>RWing</object-name>
        <object-name>LSlat</object-name>
		<object-name>spoilers-left</object-name>
		<object-name>LAileron</object-name>
		<object-name>LFlap</object-name>
		<object-name>RSlat</object-name>
		<object-name>RFlap</object-name>
		<object-name>RAileron</object-name>
		<object-name>spoilers-right</object-name>
		
		<object-name>RMainGearDoor-inner</object-name>
        <object-name>RMainGearDoor-lower</object-name>
         <object-name>LMainGearDoor-inner</object-name>
        <object-name>LMainGearDoor-lower</object-name>		 
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-wing</texture-prop>
        <texture>wings.png</texture>
    </animation>
	
	
	
		<!-- H-tail -->
    <animation>
        <type>material</type>
		<object-name>RHStab</object-name>
        <object-name>RHStab-fairing</object-name>
        <object-name>LHStab</object-name>
		<object-name>LHStab-fairing</object-name>
		
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-htail</texture-prop>
        <texture>htail.png</texture>
    </animation>
	
	<!-- V tail -->
    <animation>
        <type>material</type>
		<object-name>vstab</object-name>
        <object-name>rudder</object-name>
		<property-base>sim/model/livery</property-base>
        <texture-prop>texture-vtail</texture-prop>
        <texture>vtail.png</texture>
    </animation>
	
	<!-- pylons -->
    <animation>
        <type>material</type>
		<object-name>pylon-outer</object-name>
		<object-name>pylon-inner</object-name>
		<object-name>MER</object-name>
		<object-name>centerline</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-pylon</texture-prop>
        <texture>pylons.png</texture>
    </animation>
	

	
	<!-- tanks  -->
    <animation>
        <type>material</type>
		<object-name>RDropTank</object-name>
        <object-name>LDropTank</object-name>
        
		
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-tanks</texture-prop>
        <texture>tanks.png</texture>
    </animation>
  
<!-- bits-->
 <animation>
        <type>material</type>
		<object-name>refueling-bay-door</object-name>
       
        
		
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture-misc</texture-prop>
        <texture>misc.png</texture>
    </animation>
 
 
 <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>canopy-glass</object-name>
	<object-name>windscreen</object-name>
	<object-name>mirror</object-name>
	<object-name>hud-glass</object-name>
	
</animation>
 
 
